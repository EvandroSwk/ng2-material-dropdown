    "repository": {
        "type": "git",
        "url": "https://github.com/Gbuomprisco/ng2-material-dropdown"
    },
    "bugs": {
        "url": "https://github.com/Gbuomprisco/ng2-material-dropdown/issues"
    },
    "homepage": "https://github.com/Gbuomprisco/ng2-material-dropdown",
